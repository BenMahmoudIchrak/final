{% extends 'baseBack.html.twig' %}

{% block title %}Liste des points des relais{% endblock %}

{% block body %}
    <h1>Liste des points des relais</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id  pt</th>
                <th>addr</th>
                <th>region</th>
                <th>horaire</th>
            </tr>
        </thead>
        <tbody>
        {% for pointderelais in pointderelaiss %}
            <tr>
                <td>{{ pointderelais.idPointderelais }}</td>
                <td>{{ pointderelais.adressePointderelais }}</td>
                <td>{{ pointderelais.region }}</td>
                <td>{{ pointderelais.horaire }}</td>
                
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}
